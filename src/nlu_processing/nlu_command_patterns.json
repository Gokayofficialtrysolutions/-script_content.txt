[
  {
    "command_name": "CREATE_FILE",
    "intent_aliases": ["CREATE_FILE_INTENT", "MAKE_FILE", "NEW_FILE"],
    "patterns": [
      [
        {"LOWER": {"IN": ["create", "make", "generate", "new"]}},
        {"LOWER": {"IN": ["a", "an", "the"]}, "OP": "?"},
        {"LOWER": "file", "OP": "?"},
        {"LOWER": "named", "OP": "?"},
        {"ENT_TYPE": {"IN": ["FILE_PATH", "FILENAME", "PRODUCT"]}, "OP": "+", "CAPTURE": "file_path"}
      ],
      [
        {"LOWER": {"IN": ["create", "make"]}},
        {"ENT_TYPE": {"IN": ["FILE_PATH", "FILENAME", "PRODUCT"]}, "CAPTURE": "file_path"},
        {"LOWER": "file", "OP": "?"}
      ]
    ],
    "entity_mapping": {
      "file_path": "target_path"
    },
    "default_params": {}
  },
  {
    "command_name": "RUN_AGENT_ON_FILE",
    "intent_aliases": ["PROCESS_FILE_WITH_AGENT", "AGENT_ACTION_FILE"],
    "patterns": [
      [
        {"LOWER": {"IN": ["run", "execute", "use", "apply"]}},
        {"ENT_TYPE": "AGENT_NAME", "CAPTURE": "agent_name"},
        {"LOWER": "on"},
        {"ENT_TYPE": {"IN": ["FILE_PATH", "FILENAME"]}, "CAPTURE": "target_file"},
        {"LOWER": "to", "OP": "?"},
        {"LOWER": {"REGEX": "\\w+"}, "OP": "*", "CAPTURE": "action_description"}
      ]
    ],
    "entity_mapping": {
      "agent_name": "agent_name",
      "target_file": "file_path",
      "action_description": "task_details"
    },
    "default_params": {}
  },
  {
    "command_name": "SUMMARIZE_TEXT_COMMAND",
    "intent_aliases": ["SUMMARIZE_TEXT_INTENT"],
    "patterns": [
      [
        {"LOWER": {"IN": ["summarize", "give", "provide"]}},
        {"LOWER": {"IN": ["me", "a"]}, "OP": "?"},
        {"LOWER": "summary"},
        {"LOWER": "of", "OP": "?"},
        {"LOWER": "this", "OP": "?"},
        {"LOWER": "text", "OP": "?"},
        {"IS_PUNCT": True, "OP": "?"},
        {"TEXT": {"REGEX": ".+"}, "OP": "+", "CAPTURE": "text_to_summarize"}
      ],
      [
        {"LOWER": "summarize"},
        {"ENT_TYPE": "TEXT_BLOCK", "CAPTURE": "text_to_summarize"}
      ]
    ],
    "entity_mapping": {
      "text_to_summarize": "text_content"
    },
    "default_params": {}
  },
  {
    "command_name": "WEB_SEARCH_COMMAND",
    "intent_aliases": ["SEARCH_THE_WEB", "FIND_INFO_ONLINE"],
    "patterns": [
      [
        {"LOWER": {"IN": ["search", "find", "look up"]}},
        {"LOWER": {"IN": ["for", "online for", "on the web for"]}, "OP": "?"},
        {"LOWER": {"REGEX": ".+"}, "OP": "+", "CAPTURE": "search_query"}
      ]
    ],
    "entity_mapping": {
      "search_query": "query"
    },
    "default_params": {}
  },
  {
    "command_name": "ADD_USER_OBJECTIVE",
    "intent_aliases": ["CREATE_OBJECTIVE", "NEW_GOAL", "ADD_GOAL"],
    "patterns": [
      [
        {"LOWER": {"IN": ["add", "create", "new", "set"]}},
        {"LOWER": {"IN": ["user", "my", "long-term", "long term"]}, "OP": "*"},
        {"LOWER": {"IN": ["objective", "goal"]}},
        {"IS_PUNCT": True, "OP": "?"},
        {"LOWER": {"IN": ["called", "named", "of", "to", "that", "for"]}, "OP": "?"},
        {"TEXT": {"REGEX": ".+"}, "OP": "+", "CAPTURE": "description"}
      ],
      [
        {"LOWER": {"IN": ["my", "new"]}},
        {"LOWER": {"IN": ["objective", "goal"]}},
        {"LOWER": "is"},
        {"LOWER": "to", "OP": "?"},
        {"TEXT": {"REGEX": ".+"}, "OP": "+", "CAPTURE": "description"}
      ]
    ],
    "entity_mapping": {
      "description": "description"
    },
    "default_params": {
      "user_identifier": "default_user", "priority": 3
    }
  },
  {
    "command_name": "LIST_USER_OBJECTIVES",
    "intent_aliases": ["SHOW_OBJECTIVES", "VIEW_GOALS", "WHAT_ARE_MY_GOALS"],
    "patterns": [
      [
        {"LOWER": {"IN": ["list", "show", "view", "get", "what are"]}},
        {"LOWER": {"IN": ["my", "user", "the", "all"]}, "OP": "?"},
        {"LOWER": "active", "OP": "?", "CAPTURE": "status_active"},
        {"LOWER": "completed", "OP": "?", "CAPTURE": "status_completed"},
        {"LOWER": "on-hold", "OP": "?", "CAPTURE": "status_on_hold"},
        {"LOWER": {"IN": ["objectives", "goals"]}}
      ]
    ],
    "entity_mapping": {
        "status_active": "status_filter_active",
        "status_completed": "status_filter_completed",
        "status_on_hold": "status_filter_on_hold"
    },
    "default_params": {
      "user_identifier": "default_user"
    },
    "post_processing_logic": {
        "derive_status_filter": {
            "source_entities": ["status_filter_active", "status_filter_completed", "status_filter_on_hold"],
            "target_param": "status_filter",
            "logic": "first_truthy_value_as_status"
        }
    }
  },
  {
    "command_name": "SET_USER_OBJECTIVE_STATUS",
    "intent_aliases": ["UPDATE_OBJECTIVE_STATUS", "CHANGE_GOAL_STATUS"],
    "patterns": [
      [
        {"LOWER": {"IN": ["set", "update", "change", "mark"]}},
        {"LOWER": {"IN": ["user", "my"]}, "OP": "?"},
        {"LOWER": {"IN": ["objective", "goal"]}},
        {"ENT_TYPE": {"IN": ["OBJECTIVE_ID", "PRODUCT", "ORG", "WORK_OF_ART"]}, "CAPTURE": "objective_id"},
        {"LOWER": {"IN": ["to", "as", "status to"]}, "OP": "?"},
        {"LOWER": {"IN": ["active", "completed", "on-hold", "on_hold", "archived"]}, "CAPTURE": "new_status"}
      ]
    ],
    "entity_mapping": {
      "objective_id": "objective_id",
      "new_status": "new_status"
    },
    "default_params": {
      "user_identifier": "default_user"
    },
    "value_transformations": {
        "new_status": {"on-hold": "on_hold"}
    }
  }
]
